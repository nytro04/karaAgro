{
  "name": "karaagro",
  "version": "1.0.0",
  "description": "Agric website",
  "main": "index.js",
  "scripts": {
    "watch:sass": "node-sass sass/main.scss css/style.css -w",
    "compile:sass": "node-sass sass/main.scss css/style.comp.css",
    "concat:css": "concat -o css/style.concat.css css/font/flaticon.css css/style.comp.css",
    "prefix:css": "postcss --use autoprefixer -b 'last 10 version' css/style.concat.css -o css/style.prefix.css",
    "compress:css": "node-sass css/style.prefix.css css/style.css --output-style compressed",
    "build:css": "npm-run-all compile:sass concat:css prefix:css compress:css"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nytro04/karaAgro.git"
  },
  "author": "Francis Badasu",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/nytro04/karaAgro/issues"
  },
  "homepage": "https://github.com/nytro04/karaAgro#readme",
  "devDependencies": {
    "autoprefixer": "^9.6.1",
    "concat": "^1.0.3",
    "node-sass": "^4.12.0",
    "npm-run-all": "^4.1.5",
    "postcss-cli": "^6.1.3"
  }
}
